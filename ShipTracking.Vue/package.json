{
  "name": "shiptracking",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "watch": "vue-tsc --noEmit && vite build --watch --mode development",
    "preview": "vite preview",
    "lint": "vue-tsc --noEmit && eslint ./ && prettier --check .",
    "lint-fix": "vue-tsc --noEmit && eslint --fix ./ && prettier --check .",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "sass-embedded": "^1.83.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vue-tsc": "^2.1.10"
  }
}
